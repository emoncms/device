{
    "name": "Sensor Solar",
    "category": "Gtronica",
    "group": "Custom",
    "description": "Automatic inputs and feeds creation for gtronica.com Multi Sensors (Solar) device.",
    "inputs": [
        {
            "name": "T",
            "description": "Temperature C",
            "processList": [
